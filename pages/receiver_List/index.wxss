/* pages/receiver_List/index.wxss */
.list-wrapper{
    padding: 0 32rpx;
}
.list{
    display: flex;
    justify-content: space-between;
    height: 120rpx;
    align-items: center;
    border-bottom: 1rpx solid #E4E4E4;
}
.list-left{
    display: flex;
}
.left-text{
    flex-direction: column;
}
.left-text view{
    font-size:32rpx;
    font-family:PingFangSC;
    font-weight:400;
    color:rgba(26,29,30,1);
    line-height:32rpx;
}
.left-text view+view{
    margin-top: 16rpx;
    font-size:28rpx;
    font-family:PingFangSC;
    font-weight:400;
    color:rgba(26,29,30,.6);
    line-height:28rpx;
}
.list-left image{
    width:80rpx;
    height:80rpx;
    border-radius: 50%;
    margin-right: 24rpx;
}
.right-img image{
    width: 44rpx;
    height: 44rpx;
}