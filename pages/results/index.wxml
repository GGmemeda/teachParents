<!--pages/results/index.wxml-->
<tab tabarr="{{tabArr}}" bind:changetab="pageTabFun"></tab>
<view wx:if="{{transcriptData.length}}">
  <view class="cul">
    <view class="cli {{index == (transcriptData.length-1) ? '' : 'border-b'}}" 
          bindtap="openUrl" 
          wx:for="{{transcriptData}}" 
          wx:key="{{index}}"
          data-status="{{item.status}}"
          data-id="{{item.id}}">
      <text>{{item.examName}}</text>
      <image class="topng" mode="widthFix" src="/images/icon/to.png"></image>
    </view>
  </view>
</view>

<no-data wx:if="{{!transcriptData.length}}" morentext="{{notext}}"></no-data>

<view class="psoi" wx:if="{{!navTabIndex}}">
  <view class="cz angular" bindtap="addresults">添加考试</view>
</view>