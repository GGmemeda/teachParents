.logo {
  width: 200rpx;
  display: block;
  margin: 0 auto;
  vertical-align: top;
}

.logotext {
  font-size: 46rpx;
  color: #1A1D1E;
  text-align: center;
  font-weight: bold;
  margin: 26rpx auto 0 auto;
}

.content {
  height: 100vh;
  background-color: #fff;
}

.xieyi {
  color: rgba(44, 47, 48, .6);
  font-size: 24rpx;
  text-align: center;
  margin: 38rpx auto 120rpx auto;
}

.xieyi text {
  color: rgba(0, 155, 253, .6);
}

.czz {
  width: 590rpx;
  position: relative;
  overflow: hidden;
  color: #fff;
}

.czz button {
  position: absolute;
  opacity: 0;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
}

.top1 {
  padding-top: 140rpx;
}

.czz image {
  width: 36rpx;
  margin-right: 20rpx;
}
