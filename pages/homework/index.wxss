/* pages/my_help_center/index.wxss */
.t1 {
  height: 400rpx;
  background-color: #fff;
  padding: 32rpx;
}

.titles {
  height: 100rpx;
  background-color: #fff;
  padding: 0 32rpx;
  display: flex;
  align-items: center;
}

.titles input {
  font-size:32rpx;
  font-weight:400;
  color:rgba(26,29,30,1);
  flex: 1;
  height: 100%;
}

.t1 textarea {
  width: 100%;
  font-size: 32rpx;
  height: 300rpx;
  font-weight: 400;
}

.t1 .ct {
  color: rgba(47, 52, 68, .6);
  font-size: 32rpx;
}

.uploadimg {
  background-color: #fff;
  padding: 0 32rpx;
}

.textmax{
  color: rgba(26, 29, 30, 1);
}

.tit {
  color: rgba(47, 52, 68, 1);
  font-size: 32rpx;
  padding-top: 32rpx;
}

.addimg image {
  width: 100%;
  height: 100%;
}

.addimg {
  position: relative;
  overflow: hidden;
  border: 1rpx solid #bbbbbb;
  width: 180rpx;
  height: 180rpx;
  border-radius: 12rpx;
  background: url("data:image/png;base64,") no-repeat center/44rpx;
  margin-right: 72rpx;
  margin-bottom: 32rpx;
}

.imgcontent {
  padding: 0 0 30rpx 0;
  overflow: hidden;
  display: flex;
  flex-wrap: wrap;
}

.imgcontent .addimg:nth-child(3n) {
  margin-right: 0;
}

.botom {
  padding: 0 32rpx;
  background-color: #fff;
}

.rightangu {
  display: flex;
  align-items: center;
}

.rightangu image {
  margin-left: 6rpx;
}

.tosi {
  width: 26rpx;
}

.cantactphone {
  display: flex;
  height: 100rpx;
  align-items: center;
  background-color: #fff;
  justify-content: space-between;
}

.cantactphone > text {
  color: rgba(36, 42, 58, 1);
  font-size: 32rpx;
  width: 180rpx;
}

.cantactphone input {
  font-size: 32rpx;
  color: #242A3A;
  flex: 1;
}

.angular {
  margin-top: 60rpx;
}

.cloase {
  position: absolute;
  right: 0;
  top: 0;
  width: 40rpx;
  height: 40rpx;
  text-align: center;
  line-height: 40rpx;
  font-size: 38rpx;
}

.mini {
  zoom: .7;
}